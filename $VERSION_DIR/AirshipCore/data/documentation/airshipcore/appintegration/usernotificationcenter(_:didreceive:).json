type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"Main Thread:","type":"text"}]},{"type":"text","text":" Proceeds with the standard launch sequence, making the app’s UI active and visible."}]}]},{"content":[{"inlineContent":[{"inlineContent":[{"text":"Background Thread:","type":"text"}],"type":"strong"},{"type":"text","text":" Runs this notification code. By the time it can switch back to the main"},{"type":"text","text":" "},{"type":"text","text":"thread, the app is often already active."}],"type":"paragraph"}]}]},{"inlineContent":[{"type":"text","text":"This breaks the critical assumption that code for a “direct open” notification runs "},{"type":"emphasis","inlineContent":[{"text":"before","type":"text"}]},{"type":"text","text":" the app is fully"},{"type":"text","text":" "},{"type":"text","text":"interactive. This can lead to incorrect direct open counts."}],"type":"paragraph"},{"level":3,"text":"Migration","anchor":"Migration","type":"heading"},{"inlineContent":[{"type":"text","text":"To fix this, you must migrate to the synchronous version of this method, which accepts and forwards a "},{"type":"codeVoice","code":"completionHandler"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"This guarantees your code runs on the main thread at the correct point in the lifecycle, before the app becomes active."}],"type":"paragraph"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"text":"center: The notification center that delivered the notification.","type":"text"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"response: The user’s response to the notification.","type":"text"}]}]}]},{"content":[{"inlineContent":[{"type":"text","text":""},{"type":"codeVoice","code":"userNotificationCenter(_:didReceive:withCompletionHandler:)"}],"type":"paragraph"}],"style":"note","type":"aside","name":"See Also"}],"kind":"content"}],"variants":[{"paths":["\/documentation\/airshipcor